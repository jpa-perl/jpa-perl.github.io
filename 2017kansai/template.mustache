    <meta property="fb:app_id" content="546362158903970" />
    <meta property="og:type" content="website" />
    <meta property="og:locale" content="ja_JP" />
    <meta property="og:site_name" content="YAPC::Kansai 2017 OSAKA" />
    <meta property="og:title" content="YAPC::Kansai 2017 OSAKA{{#page-title}} - {{page-title}}{{/page-title}}" />
    <meta property="og:description" content="YAPCはYet Another Perl Conferenceの略で、Perlを軸としたITに関わる全ての人のためのカンファレンスです。" />
    <meta property="og:url" content="http://yapcjapan.org/2017kansai/" />
    <meta property="og:image" content="http://yapcjapan.org/2017kansai/assets/images/ogp_1200x600.png" />
    <meta property="og:image" content="http://yapcjapan.org/2017kansai/assets/images/ogp_1200x1200.png" />
    <meta name="referrer" content="unsafe-url" />
    <link rel="shortcut icon" href="./assets/images/favicon.ico" />
    <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Roboto:400,300,500,700,900|Asap" />
    <link rel="stylesheet" href="//fonts.googleapis.com/earlyaccess/notosansjapanese.css" />
    <link rel="stylesheet" href="//fonts.googleapis.com/icon?family=Material+Icons" />
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/css/bootstrap.min.css" />
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" />
    <link rel="stylesheet" href="./assets/css/fonts.css" />
    <link rel="stylesheet" href="./assets/css/html.css" />
    <link rel="stylesheet" media="(max-width: 899px)" href="./assets/css/html.sp.css" />
    <link rel="stylesheet" media="(min-width: 900px)" href="./assets/css/html.pc.css" />
    <link rel="stylesheet" href="./assets/css/custom.css"/>
  </head>
  <body>
    <a id="top" name="top"></a>

    <header id="header">
      <nav class="header">
        <div class="btn-menu">
          <a href="javascript:void(0)">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
        </div><!--  / .btn-menu /  -->
      </nav>
    </header>

    <nav id="menu">
      <div id="gnavi">
        <div class="innerblock">
          <ul>
            {{#menu}}
              {{#blank}}
                <li><a href="{{url}}" target="_blank">{{title}}</a></li>
              {{/blank}}
              {{^blank}}
                <li><a href="{{url}}">{{title}}</a></li>
              {{/blank}}
            {{/menu}}
          </ul>
        </div><!--  / .innerblock /  -->
      </div><!--  / #gnavi /  -->
    </nav>

    <div id="wrapper">
      <div id="lead">
        <h1>
          <a href="/"><img src="./assets/images/header_logo.png" alt="YAPC::Kansai 2017 OSAKA / ヤプシー関西 2017 大阪" /></a>
        </h1>
        <h2><img src="./assets/images/header_date.gif" alt="2017.3.4 SAT / @MOTEX HALL" class="" /></h2>
      </div><!--  / #lead /  -->

      <div id="container">
        <div class="box-top"><img src="./assets/images/box_top.gif" /></div>
        <div class="box"{{^include-buttons}} style="padding-top: 45px"{{/include-buttons}}>

          {{#include-buttons}}
            {{> buttons}}
          {{/include-buttons}}

          {{#include-about}}
            {{> about}}
          {{/include-about}}

          {{#include-news}}
            {{> news}}
          {{/include-news}}

          {{#include-tickets}}
            {{> tickets}}
          {{/include-tickets}}

          {{#include-guest}}
            {{> guest}}
          {{/include-guest}}

          {{#include-special-session}}
            {{> special-session}}
          {{/include-special-session}}

          {{#include-sponsors}}
            {{> sponsors}}
          {{/include-sponsors}}

          {{#include-access}}
            {{> access}}
          {{/include-access}}

          {{#include-staff}}
            {{> staff}}
          {{/include-staff}}

          {{#include-code-of-conduct}}
            {{> code-of-conduct}}
          {{/include-code-of-conduct}}

          {{#include-talks}}
            {{> talks}}
          {{/include-talks}}

          {{#include-timetable}}
            {{> timetable}}
          {{/include-timetable}}

          {{#include-individual-sponsors}}
            {{> individual-sponsors}}
          {{/include-individual-sponsors}}

        </div><!--  / .box /  -->
        <div class="box-bottom"><img src="./assets/images/box_bottom.gif" /></div>
      </div><!--  / #container /  -->

      <aside><div id="footer-logo">YAPC::Kansai 2017 OSAKA</div></aside>

      <footer>
        <div id="footer">
          <div class="inner">
            <div class="social-button">
              <ul>
                <li><a href="https://twitter.com/yapcjapan" title="Twitter" target="_blank"><img src="./assets/images/btn_t.png" alt="Twitter" /></a></li>
                <li><a href="https://www.facebook.com/yapcjapan/" title="Facebook" target="_blank"><img src="./assets/images/btn_f.png" alt="Facebook" /></a></li>
              </ul>
            </div><!--  / .social-button /  -->
            <address>
              <a href="http://japan.perlassociation.org/" target="_blank"><img src="./assets/images/jpa.png" alt="JPA" /></a>
              © 2017 Japan Perl Association
            </address>
          </div><!--  / .inner /  -->
        </div><!--  / #footer /  -->
      </footer>
      <div id="pagetop"><a href="#top"><i class="fa fa-caret-square-o-up"></i></a></div>
    </div><!--  / #wrappper /  -->
    {{#include-talks}}
      {{> talk-detail-modal}}
    {{/include-talks}}
    {{#include-timetable}}
      {{> talk-detail-modal}}
    {{/include-timetable}}
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.6/js/bootstrap.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/lodash.js/4.17.4/lodash.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.17.1/moment.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/vue/2.2.1/vue.min.js"></script>
    <script src="./assets/js/vendor/micro_dispatcher.js"></script>
    <script src="./assets/js/layout.js"></script>
    <script src="./assets/js/custom.js"></script>
  </body>
</html>
